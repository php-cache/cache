{
    "name":        "cache/laravel-adapter",
    "type":        "library",
    "description": "A PSR-6 cache implementation using Laravel. This implementation supports tags.",
    "keywords":    [
        "cache",
        "psr-6",
        "laravel",
        "illuminate"
    ],
    "homepage":    "http://www.php-cache.com/en/latest/",
    "license":     "MIT",
    "minimum-stability": "dev",
    "authors":     [
        {
            "name":     "Aaron Scherer",
            "email":    "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        },
        {
            "name":     "Tobias Nyholm",
            "email":    "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        },
        {
            "name":     "Daniel Bannert",
            "email":    "d.bannert@anolilab.de",
            "homepage": "https://github.com/prisis"
        },
        {
            "name": "Florian Voutzinos",
            "email": "florian@voutzinos.com",
            "homepage": "http://florian.voutzinos.com"
        }
    ],
    "require":     {
        "php":                   "^5.5 || ^7.0",
        "cache/adapter-common":  "^0.4",
        "illuminate/cache":      "5.2.* || 5.3.* || 5.4.*",
        "illuminate/filesystem": "5.2.* || 5.3.* || 5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.1",
        "cache/integration-tests": "^0.16"
    },
    "autoload":    {
        "psr-4":                 {
            "Cache\\Adapter\\Laravel\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
